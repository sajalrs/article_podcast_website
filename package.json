{
  "name": "article_podcast_website",
  "version": "1.0.0",
  "description": "Webpage that hosts blogs and podcasts",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajalrs/article_podcast_website.git"
  },
  "author": "Sajal Satyal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sajalrs/article_podcast_website/issues"
  },
  "homepage": "https://github.com/sajalrs/article_podcast_website#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "image-extensions": "^1.1.0",
    "is-url": "^1.2.4",
    "mongoose": "^5.10.2",
    "node-fetch": "^2.6.1",
    "slate-history": "^0.58.4"
  },
  "devDependencies": {
    "concurrently": "^4.1.2"
  }
}
